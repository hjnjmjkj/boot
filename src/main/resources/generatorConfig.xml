<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="E:/software/db2/lib/db2jcc_license_cu.jar" />
	<classPathEntry
		location="E:/software/db2/lib/db2jcc.jar" />

	<context id="context1" targetRuntime="MyBatis3">
		<commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>	
	
		<jdbcConnection driverClass="com.ibm.db2.jcc.DB2Driver"
			connectionURL="jdbc:db2://192.168.8.103:60080/hisdb" userId="dev_dba"
			password="dev_dba" />
			
		<javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
 
        <!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建  使用Maven生成在target目录下，会自动创建） -->
        <javaModelGenerator targetPackage="com.fuiou.web.manager.mapper.db2.bean" targetProject="MAVEN">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!--生成SQLMAP文件 -->
        <sqlMapGenerator targetPackage="com.fuiou.web.manager.mapper.db2.hisdb"  targetProject="MAVEN" >
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>
        <!--生成Dao文件 可以配置 type="XMLMAPPER"生成xml的dao实现  context id="DB2Tables" 修改targetRuntime="MyBatis3"  -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.fuiou.web.manager.mapper.db2.hisdb"  targetProject="MAVEN" >
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

		<table tableName="T_PPC_USER_INF" enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
		</table>
		
		
	</context>
</generatorConfiguration>